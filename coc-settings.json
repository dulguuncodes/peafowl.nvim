{
  "languageserver": {
    "go": {
      "command": "gopls",
      "rootPatterns": ["go.mod"],
      "trace.server": "verbose",
      "filetypes": ["go"]
    }
  },

  "rpc.idleTimeout": 1800,
  "rpc.workspaceElapsedTime": true,
  "rpc.detailsViewing": "Working on {workspace}. {problems} errors to fix",
  "coc.preferences.messageLevel": "error"
}

